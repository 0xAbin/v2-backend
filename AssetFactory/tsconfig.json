{
  "extends": "@graphprotocol/graph-ts/types/tsconfig.base.json",
  "include": ["src", "tests"],
  "suppressImplicitAnyIndexErrors": true,
  "skipLibCheck": true,
  "noImplicitAny": false,
  "lib": ["es2019", "dom"],
  "compilerOptions": {
